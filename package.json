    },
    "homepage": "https://github.com/nawin9/EpiApi#readme",
    "dependencies": {
        "bcrypt-nodejs": "^0.0.3",
        "dotenv": "^5.0.1",
        "elastic-apm-node": "^1.2.1",
        "jsonwebtoken": "^8.2.0",
        "kcors": "^2.2.1",
        "koa": "^2.5.0",
        "koa-bodyparser": "^4.2.0",
        "koa-convert": "^1.2.0",
